<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_auth"
    app:startDestination="@id/authFragment">

    <fragment
        android:id="@+id/authFragment"
        android:name="uz.dostonbek.variantapplication.ui.AuthFragment"
        android:label="fragment_auth"
        tools:layout="@layout/fragment_auth" >
        <action
            android:id="@+id/action_authFragment_to_lockFragment"
            app:destination="@id/lockFragment" />
    </fragment>
    <fragment
        android:id="@+id/lockFragment"
        android:name="uz.dostonbek.variantapplication.ui.lockView.LockFragment"
        android:label="fragment_lock"
        tools:layout="@layout/fragment_lock" />
</navigation>